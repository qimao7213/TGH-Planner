<!-- 
  Simulate a carlike robot with the teb_local_planner in stage:
  - stage
  - map_server
  - move_base
  - static map
  - amcl
  - rviz view
 -->
<launch>

        <!--  ************** Global Parameters ***************  -->
        <!-- <param name="/use_sim_time" value="true"/> -->

        <!--  ************** Navigation ***************  -->
	<node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
  	  	<rosparam file="$(find teb_nav)/config/costmap_common_params.yaml" command="load" ns="global_costmap" />
  	 	<rosparam file="$(find teb_nav)/config/costmap_common_params.yaml" command="load" ns="local_costmap" />
  		<rosparam file="$(find teb_nav)/config/local_costmap_params.yaml" command="load" />
  		<rosparam file="$(find teb_nav)/config/global_costmap_params.yaml" command="load" />
  		<rosparam file="$(find teb_nav)/config/teb_local_planner_params.yaml" command="load" />

		<!-- <param name="base_global_planner" value="global_planner/GlobalPlanner" /> -->
		<param name="base_global_planner" value="navfn/NavfnROS" />
		<param name="planner_frequency" value="10.0" />
		<!-- <param name="planner_patience" value="5.0" /> -->

		<param name="base_local_planner" value="teb_local_planner/TebLocalPlannerROS" />
		<param name="controller_frequency" value="20.0" />
		<param name="controller_patience" value="15.0" />


        <!-- <param name="clearing_rotation_allowed" value="false" />  -->
	</node>

	  <!--  **************** Visualisation ****************  -->
	<!-- <node name="rviz" pkg="rviz" type="rviz" args="-d $(find teb_local_planner_tutorials)/cfg/rviz_navigation.rviz"/> -->

</launch>
